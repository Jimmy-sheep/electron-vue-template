{
  "name": "electron-vue-template",
  "version": "0.0.0",
  "author": "Jimmy-sheep",
  "description": "This is a template to developing an electron application with Vue 3 and TypeScript in Vite.",
  "type": "module",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jimmy-sheep/electron-vue-template.git"
  },
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build && electron-builder",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --fix",
    "visualizer": "vite-bundle-visualizer"
  },
  "dependencies": {
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "3.1.2",
    "@vitest/eslint-plugin": "^1.1.43",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.25.1",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jsdom": "^26.1.0",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-vue-devtools": "^7.7.6",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  }
}
